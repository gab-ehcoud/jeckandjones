//execute a whoami command on the agent
pipeline {    
    agent any
    stages {     
        stage("Deploy")
        {
            steps {
                withCredentials([string(credentialsId: 'team-hook', variable: 'team-hook-token')]) {
                    office365ConnectorSend webhookUrl: '''${team-hook-token}''',
                    message: 'Code is deployed',
                    status: 'Success'            
                    sh 'whoami'
            }
        }
    }
}
}